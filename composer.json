{
  "name": "usbac/wolff-framework",
  "description": "Ridiculously small PHP framework",
  "keywords": [
    "framework", "php", "small", "web", "apps", "fast", "full", "db"
  ],
  "type": "project",
  "version": "2.8.1",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/Usbac/Wolff/issues",
    "source": "https://github.com/Usbac/Wolff",
    "wiki": "https://github.com/Usbac/Wolff/wiki",
    "email": "contact@getwolff.com"
  },
  "authors": [
    {
      "name": "Usbac",
      "homepage": "http://www.usbac.com.ve",
      "email": "contacto@usbac.com.ve"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "files": [
      "system/config.php",
      "system/stdlib/config.php",
      "system/stdlib/utilities.php"
    ],
    "psr-4": {
      "Core\\": "system/core/",
      "Definitions\\": "system/definitions",
      "Utilities\\": "system/utilities/",
      "App\\": "app/",
      "Middleware\\": "app/middlewares/",
      "Controller\\": "app/controllers/"
    }
  }
}
